<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NominalCode__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Opportunity__r.Type, "Marketing Funding/Cost"),
  "7700",
  CASE(CompanyAbbr__c,
    "SOL", "4601",
    "PLM", "4401",
    "4000")
)</formula>
