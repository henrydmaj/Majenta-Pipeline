<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RenewalDate__c</fullName>
    <externalId>false</externalId>
    <formula>DATE(
  YEAR(StartDate__c+(((FLOOR(AgeMonths__c/Length__c)*Length__c)+Length__c)*30.5)),
  MONTH(StartDate__c+(((FLOOR(AgeMonths__c/Length__c)*Length__c)+Length__c)*30.5)),
  DAY(StartDate__c)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Renewal Date (Formula)</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
