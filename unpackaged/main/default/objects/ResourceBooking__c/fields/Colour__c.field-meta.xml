  CASE( TEXT(Category__c),
    'Consultancy', '#00ff00',
    'Helpdesk', '',
    'Holiday', '#0070c0',
    'Meetings', '',
    'Pre-Sales', '#ffff00',
    'Training', '#92d050',
    '#c0c0c0'
  )
  ,'#ff0000'
)</formula>
