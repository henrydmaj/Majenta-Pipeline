<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_First_name</fullName>
    <active>false</active>
    <description>No first name.</description>
    <errorConditionFormula>LEN(FirstName) &lt; 1</errorConditionFormula>
    <errorDisplayField>FirstName</errorDisplayField>
    <errorMessage>You must specify a first name!</errorMessage>
</ValidationRule>
