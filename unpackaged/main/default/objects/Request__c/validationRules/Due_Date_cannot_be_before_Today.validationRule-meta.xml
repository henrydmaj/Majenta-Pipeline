<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Due_Date_cannot_be_before_Today</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
  ISNEW(),
  DueDate__c &lt;= TODAY()
)</errorConditionFormula>
    <errorMessage>The Due Date must be a date after today's date.</errorMessage>
</ValidationRule>
