<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ProjectPercentComplete__c</fullName>
    <externalId>false</externalId>
    <formula>IF( MilestonesSum__c &lt;=0,
 TaskHoursPercentComplete__c,
  (MilestonesPercentComplete__c+TaskHoursPercentComplete__c)/2
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Project Percent Complete</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
