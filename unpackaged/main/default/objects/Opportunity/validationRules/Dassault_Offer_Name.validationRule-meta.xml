<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Dassault_Offer_Name</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(Partner__c, "Dassault"),
ISPICKVAL(DS_Offer_Name__c, ""))</errorConditionFormula>
    <errorDisplayField>DS_Offer_Name__c</errorDisplayField>
    <errorMessage>Dassault Offer Name field cannot be blank on a Dassault Opportunity</errorMessage>
</ValidationRule>
