<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Finish_Date_Not_Before_Start_Date</fullName>
    <active>true</active>
    <errorConditionFormula>FinishDate__c &lt; StartDate__c</errorConditionFormula>
    <errorDisplayField>FinishDate__c</errorDisplayField>
    <errorMessage>The finish date cannot be before the start date.</errorMessage>
</ValidationRule>
