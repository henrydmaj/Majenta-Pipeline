<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Passport_Number</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK( Passport_Expiry_Date__c )),
ISBLANK( Passport_Number__c )
)</errorConditionFormula>
    <errorDisplayField>Passport_Number__c</errorDisplayField>
    <errorMessage>If Passport Expiry Date is populated, Passport Number must be populated</errorMessage>
</ValidationRule>
