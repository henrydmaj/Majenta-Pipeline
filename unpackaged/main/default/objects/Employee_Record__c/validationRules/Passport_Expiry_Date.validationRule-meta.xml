<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Passport_Expiry_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK( Passport_Number__c )), 
ISBLANK( Passport_Expiry_Date__c ) 
)</errorConditionFormula>
    <errorDisplayField>Passport_Expiry_Date__c</errorDisplayField>
    <errorMessage>If Passport Number is populated, Passport Expiry Date must be populated.</errorMessage>
</ValidationRule>
