<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Superuser_1_Superuser_2_cant_be_same</fullName>
    <active>true</active>
    <description>Superuser 1 &amp; Superuser 2 can't be same.</description>
    <errorConditionFormula>AND(
				!ISBLANK(Site_Super_User_1__c),
				!ISBLANK(Site_Super_User_2__c),
				Site_Super_User_1__c = Site_Super_User_2__c
)</errorConditionFormula>
    <errorDisplayField>Academy_Marketing__pc</errorDisplayField>
    <errorMessage>Site Super User 1 &amp; Site Super User 2 are same.</errorMessage>
</ValidationRule>
