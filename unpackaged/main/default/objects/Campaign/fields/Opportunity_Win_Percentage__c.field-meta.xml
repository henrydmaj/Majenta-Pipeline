<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Opportunity_Win_Percentage__c</fullName>
    <externalId>false</externalId>
    <formula>IF( NumberOfWonOpportunities &lt;&gt; 0, NumberOfWonOpportunities /  ( Total_Campaign_Members__c ), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Opportunity Win %</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Percent</type>
</CustomField>
